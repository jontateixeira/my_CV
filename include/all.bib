@inproceedings{silva2018moving,
	address = {São Paulo, SP},
	title = {Moving boundary in non-equilibrium simple batch reactive distillation in a non-ideal mixture isobutene/methanol/mtbe},
	url = {http://www.proceedings.blucher.com.br/article-details/29055},
	doi = {10.5151/cobeq2018-PT.0693},
	abstract = {This article is concerned with the dynamic behavior of a reactive batch distillation process using the irreversible model. The composition trajectories in reactive residue curve map (RRCM) were calculated for a methanol/isobutene/MTBE mixture. The simulation results showed that: residue curves, including the simple reactive distillation boundaries (separatrices), were sensitive to the irreversible model applied, that the shape of residue curves depends on parameters Damköhler number (Da), molar flow rate at which the vapor is withdrawn (q) and interfacial area (A). It was observed the phenomenon of dynamic azeotropy: changes of position in stable nodes, saddles and separatrices. Applying the irreversible model it was possible observed different binary and ternary azeotropes and separatrices in non-ideal mixture.},
	language = {en},
	urldate = {2019-11-01},
	booktitle = {Blucher {Chemical} {Engineering} {Proceedings}},
	publisher = {Editora Blucher},
